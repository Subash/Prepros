{
	"name": "Prepros",
	"version": "1.0.0",
	"manifest_version": 2,
	"description": "Live refresh extension for Prepros App",
	"permissions": ["tabs", "<all_urls>"],
	"offline_enabled": true,
	"minimum_chrome_version": "18",
	"background": {
		"scripts": [ "scripts/underscore.js", "scripts/background.js"]
	},
	"icons": {
		"16": "img/icon.png",
		"48": "img/icon.png",
		"128": "img/icon.png"
	}
}